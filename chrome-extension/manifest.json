{
 "name": "Tweet Talk",
 "description": "Explain what your extension does",
 "version": "1.0",
  "manifest_version": 2,

 "permissions": ["tabs", "http://*/*", "background"],
 "background": "background.html",
 "content_scripts": [{"matches": ["http://*/*"],"js": ["twitter.js"],"css":["twitter.css"]}],
 "browser_action": {"default_icon": "16x16.png","default_popup": "popup.html"}
}
